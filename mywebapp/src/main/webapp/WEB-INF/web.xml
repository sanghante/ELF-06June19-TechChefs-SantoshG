<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>myFirstServlet</servlet-name>
  	<servlet-class>com.techchefs.mywebapp.servlets.MyFirstServlet</servlet-class>  
  </servlet>
  <servlet>
  	<servlet-name>myEmployeeSearchServlet</servlet-name>
  	<servlet-class>com.techchefs.mywebapp.servlets.EmployeeSearchServlet</servlet-class>  
  </servlet>
  <servlet-mapping>
  	<servlet-name>myEmployeeSearchServlet</servlet-name>
  	<url-pattern>/empsearch</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>myFirstServlet</servlet-name>
  	<url-pattern>/currentDateTime</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>/mywebapp/index.html</welcome-file>
  </welcome-file-list>
</web-app>
